
RANDFILE = /etc/thomsoft-cloud/api/certificate.rand

[ req ]
default_bits = 1024
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no

[ req_dn ]
C=NL
ST=NH
L=Amsterdam
O=Thomsoft Cloud API
OU=Automatically-generated Thomsoft Cloud API SSL key
CN=localhost
emailAddress=postmaster@example.com


[ cert_type ]
nsCertType = server
